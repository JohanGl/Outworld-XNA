<Window x:Class="NetworkTool.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="MainWindow" Height="350" Width="525" MinHeight="200" MinWidth="400">
    <Grid>
		<Button x:Name="ButtonPlayback" Content="Playback" Width="70" Click="ButtonPlayback_Click" Margin="421,12,0,0" HorizontalAlignment="Left" HorizontalContentAlignment="Center" VerticalAlignment="Top" Height="52" IsEnabled="False" />
		<ComboBox Name="comboSelectedFile" Height="23" Margin="12,12,0,0" VerticalAlignment="Top" HorizontalAlignment="Left" Width="403" SelectionChanged="comboSelectedFile_SelectionChanged" />
		<ComboBox Name="comboSelectedClient" Height="23" HorizontalAlignment="Left" Margin="12,41,0,0" VerticalAlignment="Top" Width="403" SelectionChanged="comboSelectedClient_SelectionChanged" />
	</Grid>
</Window>
